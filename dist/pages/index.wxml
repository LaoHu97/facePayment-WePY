  </view>
  <view class="btn-area">
    <button type="primary" bindtap="addCard">注册新会员</button>
  </view>
  <view class="btn-area">
    <button bindtap="bindCard">老会员绑定</button>
  </view>
  
<view class="foot">西安万鼎网络科技</view>

</view>